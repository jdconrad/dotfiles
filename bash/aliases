
alias ll='ls -l'
alias rm='rm -i'       # always ask before removing a file
alias mv='mv -i'       # always ask before moving a file onto another file
alias cp='cp -i'       # always ask before copying a file onto another file
alias h='history'
alias hs='history | grep -i'

alias py3='python3'

alias jdk7='export JAVA_HOME=${JDK7_HOME:?"JDK7_HOME is not set"}'
alias jdk8='export JAVA_HOME=${JDK8_HOME:?"JDK8_HOME is not set"}'
alias jdk8ea='export JAVA_HOME=${JDK8_EA_HOME:?"JDK8_EA_HOME is not set"}'
alias jdk9ea='export JAVA_HOME=${JDK9_EA_HOME:?"JDK9_EA_HOME is not set"}'

